{
    "name": "slim/slim-skeleton",
    "description": "Appeto is a powerfull app builder :)",
    "keywords": ["microframework", "rest", "router", "psr7"],
    "homepage": "https://appeto.ir/",
    "license": "MIT",
    "authors": [
        {
            "name": "Peyman Aslani",
            "email": "info@appeto.ir",
            "homepage": "https://www.appeto.ir/"
        },
        {
            "name": "Iman Aslani",
            "email": "info@appeto.ir",
            "homepage": "https://www.appeto.ir/"
        }
    ],
    "require": {
        "php": "7.4",
        "ext-json": "*",
        "ext-mongodb": "*",
        "ext-openssl": "*",
        "akrabat/ip-address-middleware": "^1.0",
        "aws/aws-sdk-php": "3.200",
        "barryvdh/elfinder-flysystem-driver": "^0.3.0",
        "cocur/slugify": "^3.2",
        "endroid/qr-code": "^2.5",
        "google/apiclient": "^2.0",
        "google/auth": "^1.37",
        "league/flysystem-aws-s3-v3": "1.0.29",
        "league/oauth2-instagram": "0.1.*",
        "matthiasmullie/minify": "^1.3",
        "melipayamak/php": "^0.1.0",
        "mongodb/mongodb": "^1.4.3",
        "monolog/monolog": "^1.17",
        "phpmailer/phpmailer": "^6.0",
        "predis/predis": "^2.3",
        "pusher/pusher-php-server": "^7.0",
        "slim/http-cache": "^0.4.0",
        "slim/php-view": "^2.0",
        "slim/slim": "^3.1",
        "studio-42/elfinder": "^2.1",
        "tecnickcom/tcpdf": "^6.2.13",
        "true/punycode": "~2.0",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    },
    "autoload": {
        "psr-4": {
            "Panel\\": "src/Panel",
            "Models\\": "src/Models",
            "Tools\\": "src/Tools",
            "Api\\": "src/Api",
            "Export\\": "src/Export"
        }
    }
}
